<Project Sdk="Microsoft.NET.Sdk.Razor">

  <Import Project="..\..\..\..\common.props" />

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <AssemblyName>Generic.Abp.Metro.UI.Packages</AssemblyName>
    <PackageId>Generic.Abp.Metro.UI.Packages</PackageId>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
    <IsPackable>true</IsPackable>
    <OutputType>Library</OutputType>
    <RootNamespace />
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.AspNetCore.Mvc.UI.Bundling.Abstractions" Version="$(AbpVersion)" />
    <PackageReference Include="Volo.Abp.Localization" Version="$(AbpVersion)" />
  </ItemGroup>

</Project>
